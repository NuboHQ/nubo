{
  "name": "nubo",
  "version": "0.0.0-alpha",
  "description": "",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all clean build:client",
    "build:client": "esbuild src/client/index.tsx --bundle --jsx=automatic --minify --outfile=.nubo/app.js",
    "build:dev": "npm-run-all clean build:client:dev",
    "build:client:dev": "esbuild src/client/index.tsx --bundle --watch --jsx=automatic --sourcemap --outfile=.nubo/app.js",
    "de2v": "clear && bun --hot src/main.tsx",
    "dev": "clear && concurrently 'bun --hot src/main.tsx' 'bun run build:dev'",
    "devw": "webpack serve --hot",
    "clean": "rimraf .nubo"
  },
  "dependencies": {
    "baojs": "^0.2.1",
    "bun-types": "^0.2.2",
    "hono": "^2.5.4",
    "htm": "^3.1.1",
    "preact": "^10.11.3",
    "preact-render-to-string": "^5.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "vm": "^0.1.0",
    "webpack-dev-middleware": "^6.0.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@swc/core": "^1.3.19",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "concurrently": "^7.6.0",
    "core-js": "^3.26.1",
    "esbuild": "^0.15.15",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "react-refresh": "^0.14.0",
    "rimraf": "^3.0.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  }
}
