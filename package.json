{
  "name": "nubo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "generate": "turbo run generate",
    "graphql:generate": "turbo run graphql:generate",
    "db:push": "turbo run db:push",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docker": "npm run docker:build && npm run docker:run",
    "docker:build": "docker build -t gcr.io/nubo-development/nubojs-api:0.0.1 .",
    "docker:run": "docker run -d -p 5003:5003 gcr.io/nubo-development/nubojs-api:0.0.1",
    "docker:push": "docker push gcr.io/nubo-development/nubojs-api:0.0.1"
  },
  "dependencies": {
    "graphql": "15.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "eslint-config-custom": "*",
    "npm-run-all": "^4.1.5",
    "prettier": "latest",
    "ts-node": "^10.9.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.18"
}
