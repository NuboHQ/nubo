generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["deno"]
  output          = "../generated/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Log {
  id      Int    @id @default(autoincrement())
  level   Level
  message String
  meta    Json
}

enum Level {
  Info
  Warn
  Error
}

model City {
  id         Int    @id @default(autoincrement())
  city       String
  city_ascii String
  lat        Float
  lng        Float
  country    String
  iso2       String
  iso3       String
  admin_name String
  capital    String
  population Int?

  @@map("cities")
}
