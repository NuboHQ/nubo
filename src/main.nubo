---
import { FC } from 'react';

const id = 1;
const result = await fetch(`https://dummyjson.com/products/${id}`);
const product = await result.json();

export const config = { path: '/logs', props: { id, product } };
---

const Page: FC = () => {
  return (
    <div>
      <h1>{product.title}</h1>

      <p>{product.description}</p>
    </div>
  );
};

export default Page;
