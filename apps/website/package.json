{
  "name": "@nubojs/website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --port 5555",
    "start": "node server/server.mjs",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rimraf dist",
    "docker": "npm run docker:build && npm run docker:run",
    "docker:build": "docker build -t nubodev/nubo:0.0.1 .",
    "docker:run": "docker run -d -p 5555:5555 nubodev/nubo:0.0.1",
    "docker:push": "docker push nubodev/nubo:0.0.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@nubojs/maths": "*",
    "@nubojs/ui": "*"
  },
  "devDependencies": {
    "@astrojs/node": "^2.0.0",
    "@astrojs/react": "^1.2.0",
    "@astrojs/tailwind": "^2.1.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "astro": "^1.5.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24"
  }
}
